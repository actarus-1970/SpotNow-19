modules = ["web", "nodejs-20", "postgresql-16"]
[agent]
expertMode = true
integrations = ["javascript_database:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["imagemagick"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "dev"

[[workflows.workflow]]
name = "dev"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 3001
externalPort = 3001

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 5001
externalPort = 3000

[[ports]]
localPort = 33979
externalPort = 5173

[[ports]]
localPort = 35973
externalPort = 5000

[[ports]]
localPort = 37107
externalPort = 6000

[[ports]]
localPort = 37913
externalPort = 3003

[[ports]]
localPort = 38151
externalPort = 6800

[[ports]]
localPort = 39379
externalPort = 3002

[[ports]]
localPort = 43159
externalPort = 4200

[deployment]
deploymentTarget = "vm"
run = ["npm", "start"]
build = ["npm", "run", "build"]
